{
  "name": "yt-likes-listener-backend",
  "version": "1.0.0",
  "description": "Backend Netlify Functions pour automatiser le téléchargement d'audio des vidéos YouTube aimées",
  "main": "index.js",
  "scripts": {
    "build": "npm run lint && tsc",
    "dev": "netlify dev",
    "start": "netlify dev",
    "lint": "eslint services/*.ts common/*.ts netlify/functions/**/*.mts",
    "lint:fix": "eslint 'common/*.ts' services/*.ts --ext .ts --fix",
    "test": "echo \"Tests à implémenter\" && exit 0",
    "test:watch": "echo \"Tests watch à implémenter\" && exit 0",
    "deploy": "netlify deploy",
    "deploy:prod": "netlify deploy --prod"
  },
  "keywords": ["youtube", "audio", "download", "netlify", "firebase", "fcm"],
  "author": "YT-Likes-Listener",
  "license": "MIT",
  "dependencies": {
    "@netlify/functions": "^2.4.1",
    "firebase-admin": "^12.0.0",
    "googleapis": "^126.0.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "typescript": "^5.2.2",
    "@types/node": "^20.8.0"
  }
}